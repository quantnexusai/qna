#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules/tailwindcss/lib/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules/tailwindcss/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules/tailwindcss/lib/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules/tailwindcss/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.10.11_@swc+helpers@0.5.15__@types+node@22.12.0_typescript@5.7.3_/node_modules:/Users/ariharrison/Documents/GitHub/qna/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../tailwindcss/lib/cli.js" "$@"
fi
