#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/playwright@1.50.0/node_modules/playwright/node_modules:/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/playwright@1.50.0/node_modules:/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/playwright@1.50.0/node_modules/playwright/node_modules:/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/playwright@1.50.0/node_modules:/Users/ariharrison/Documents/GitHub/ApexM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../playwright@1.50.0/node_modules/playwright/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../playwright@1.50.0/node_modules/playwright/cli.js" "$@"
fi
